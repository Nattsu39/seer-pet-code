{
  "name": "@seerbp/petcode-sdk",
  "version": "1.0.2",
  "description": "PetCode SDK for TypeScript",
  "license": "MIT",
  "author": "Nattsu39 <Natsu39@outlook.com> (https://github.com/Nattsu39)",
  "keywords": [
    "seer",
    "petcode",
    "sdk",
    "typescript",
    "protobuf"
  ],
  "bugs": {
    "url": "https://github.com/Nattsu39/seer-pet-code/issues"
  },
  "homepage": "https://github.com/Nattsu39/seer-pet-code/#readme",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*.js": "./dist/*.js",
    "./dist/*.js": "./dist/*.js",
    "./pb/*.js": "./dist/generated/seerbp/petcode/*.js"
  },
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "README*.md"
  ],
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build",
    "clean": "npx rimraf ./dist",
    "build": "npm run clean && npx tsc -b -clean && npx tsc -b",
    "test": "node --import tsx --test"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/pako": "^2.0.4",
    "rimraf": "^6.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.10.2",
    "pako": "^2.1.0"
  }
}
